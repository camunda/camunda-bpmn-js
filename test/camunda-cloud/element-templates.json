          "linkName": "After",
          "property": "bindingType"
        },
        "condition": {
          "property": "postRun",
          "equals": true
        },
        "choices": [
          {
            "name": "Latest",
            "value": "latest"
          },
          {
            "name": "Deployment",
            "value": "deployment"
          },
          {
            "name": "Version Tag",
            "value": "versionTag"
          }
