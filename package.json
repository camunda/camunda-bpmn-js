{
  "name": "camunda-bpmn-js",
  "version": "1.3.1",
  "description": "Embeddable Camunda modeling distributions based on bpmn-js",
  "main": "lib/index.js",
  "scripts": {
    "all": "run-s clean lint test distro test:distro",
    "lint": "eslint .",
    "dev": "npm test -- --auto-watch --no-single-run",
    "test": "karma start karma.config.js",
    "clean": "del-cli dist",
    "distro": "rollup -c --failAfterWarnings",
    "format": "run-s format:markdown 'lint -- --fix'",
    "format:markdown": "remark . -qo",
    "test:distro": "node tasks/test-distro.js",
    "start:base": "cross-env SINGLE_START=base-modeler npm run dev",
    "start:platform": "cross-env SINGLE_START=camunda-platform-modeler npm run dev",
    "start:cloud": "cross-env SINGLE_START=camunda-cloud-modeler npm run dev",
    "start:platform-viewer": "cross-env SINGLE_START=camunda-platform-viewer npm run dev",
    "start:cloud-viewer": "cross-env SINGLE_START=camunda-cloud-viewer npm run dev",
    "start:platform-navigated-viewer": "cross-env SINGLE_START=camunda-platform-navigated-viewer npm run dev",
    "start:cloud-navigated-viewer": "cross-env SINGLE_START=camunda-cloud-navigated-viewer npm run dev",
    "start:cloud-connectors-extension": "cross-env SINGLE_START=camunda-cloud-connectors-extension npm run dev",
    "prepublishOnly": "run-s test:distro",
    "prepare": "run-s clean distro"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/camunda/camunda-bpmn-js"
  },
  "keywords": [
    "bpmn",
    "bpmn20",
    "camunda",
    "zeebe",
    "modeling",
    "bpmn-js"
  ],
  "author": {
    "name": "Niklas Kiefer",
    "url": "https://github.com/pinussilvestrus"
  },
  "contributors": [
    {
      "name": "bpmn.io contributors",
      "url": "https://github.com/bpmn-io"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "@bpmn-io/align-to-origin": "^0.7.0",
    "@bpmn-io/element-templates-icons-renderer": "^0.3.0",
    "bpmn-js": "^11.3.1",
    "bpmn-js-color-picker": "^0.5.0",
    "bpmn-js-executable-fix": "^0.2.0",
    "camunda-bpmn-js-behaviors": "^0.4.0",
    "camunda-bpmn-moddle": "^7.0.1",
    "diagram-js": "^11.9.0",
    "diagram-js-minimap": "^4.0.1",
    "diagram-js-origin": "^1.4.0",
    "inherits-browser": "^0.1.0",
    "min-dash": "^4.0.0",
    "zeebe-bpmn-moddle": "^0.17.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@bpmn-io/element-template-chooser": "^1.0.0",
    "@bpmn-io/properties-panel": "^1.3.1",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "babel-loader": "^9.0.0",
    "babel-plugin-istanbul": "^6.1.1",
    "bpmn-js-connectors-extension": "^0.4.5",
    "bpmn-js-properties-panel": "^1.17.1",
    "chai": "^4.3.7",
    "cross-env": "^7.0.3",
    "del-cli": "^5.0.0",
    "downloadjs": "^1.4.7",
    "eslint": "^8.24.0",
    "eslint-plugin-bpmn-io": "^1.0.0",
    "execa": "^5.0.0",
    "file-drops": "^0.5.0",
    "karma": "^6.4.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage": "^2.2.0",
    "karma-debug-launcher": "0.0.5",
    "karma-env-preprocessor": "^0.1.1",
    "karma-firefox-launcher": "^2.1.2",
    "karma-mocha": "^2.0.1",
    "karma-safari-launcher": "^1.0.0",
    "karma-sinon-chai": "^2.0.2",
    "karma-webpack": "^5.0.0",
    "min-dom": "^4.1.0",
    "mocha": "^10.1.0",
    "mocha-test-container-support": "^0.2.0",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^19.4.0",
    "raw-loader": "^4.0.2",
    "remark-cli": "^11.0.0",
    "remark-preset-bpmn-io": "^0.2.1",
    "rollup": "^3.0.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-terser": "^7.0.2",
    "sinon": "^15.0.0",
    "sinon-chai": "^3.7.0",
    "webpack": "^5.74.0"
  },
  "peerDependencies": {
    "bpmn-js-properties-panel": ">= 1.11.3"
  },
  "files": [
    "dist",
    "lib",
    "styles",
    "util"
  ],
  "remarkConfig": {
    "plugins": [
      "preset-bpmn-io",
      [
        "lint-no-html",
        false
      ]
    ]
  }
}
